pipeline {
    agent none
    stages {
        stage('Back-end') {
            agent {
                docker { image 'ubuntu:latest' }
            }
            steps {
                sh 'uname'
            }
        }
        stage('Front-end') {
            agent {
                docker { image 'jenkins:latest' }
            }
            steps {
                sh 'jenkins --version'
            }
        }
    }
}