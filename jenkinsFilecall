pipeline {
agent any
stages {
        stage('Build') {
            steps {
                echo 'Building..'
				sh 'sudo -s; ./build'
				
            }
        }
        stage('deploy') {
            steps {
                echo 'Deploying to TEST environment..'
				sh 'ls -lrt; ./deploy'
            }
        }
        stage('test') {
            steps {
                echo 'Testing....'
				sh 'sudo -s; ./test'
					}
				}
    }
}